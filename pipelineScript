node {
    stage('Clone repo') {
        git branch: 'main', url: 'https://github.com/robertp048/jenkinsTng.git'
    }
    stage('List repo contents') {
        sh 'ls -alR'
    }
    stage('Print selected file') {
        sh 'cat seed2.json'
    }
    stage('Send notification') {
        sh 'echo "Pipeline processed successfully!"'
    }
}
